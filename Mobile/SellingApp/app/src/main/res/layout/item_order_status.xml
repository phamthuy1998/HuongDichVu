<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="orderStatus"
            type="thuypham.n16dccn159.ptithcm.sellingapp.data.OrderStatus" />
    </data>

    <TextView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/itemOrderStatus"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:background="@android:color/white"
        android:drawableEnd="@drawable/ic_gray_right_arrow"
        android:padding="16dp"
        android:text="@{orderStatus.statusName}"
        android:textColor="@android:color/black"
        android:textSize="@dimen/ts_name"
        tools:text="@string/tv_taken" />
</layout>